SELECT A.SNAME,B.CITY FROM SALESPEOPLE A, SALESPEOPLE B
WHERE A.CITY=B.CITY
AND A.SNAME!=B.SNAME;
-------------------------------------------------------------------------------------------------------
SELECT CNAME,CITY FROM CUSTOMERS
WHERE RATING=
(SELECT RATING FROM CUSTOMERS
WHERE CNAME='Hoffman');
