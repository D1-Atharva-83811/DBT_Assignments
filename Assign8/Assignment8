-------------------------------------------------------------------------------------------------------
SELECT ONUM, SNUM, ROUND((AMT*0.12),2) "COMMISSION" FROM ORDERS;
-------------------------------------------------------------------------------------------------------
SELECT CONCAT('FOR THE CITY ',UPPER(CITY),' THE HIGHEST RATING IS'), MAX(RATING)
    -> FROM CUSTOMERS
    -> GROUP BY CITY;
-------------------------------------------------------------------------------------------------------
SELECT RATING,CNAME,CNUM FROM CUSTOMERS
    -> ORDER BY RATING DESC;
-------------------------------------------------------------------------------------------------------
SELECT ODATE, COUNT(*) FROM ORDERS GROUP BY ODATE ORDER BY COUNT(*) DESC;
-------------------------------------------------------------------------------------------------------
