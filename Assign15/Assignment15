INSERT INTO MULTICUST
SELECT SALESPEOPLE.SNUM,SNAME,SALESPEOPLE.CITY,COMM FROM CUSTOMERS,SALESPEOPLE
WHERE CUSTOMERS.SNUM=SALESPEOPLE.SNUM
GROUP BY SALESPEOPLE.SNUM,SNAME,SALESPEOPLE.CITY,COMM 
HAVING COUNT(CUSTOMERS.SNUM)>1;
-------------------------------------------------------------------------------------------------------
DELETE FROM CUSTOMERS
WHERE NOT EXISTS
(SELECT CNUM FROM ORDERS
WHERE ORDERS.CNUM=CUSTOMERS.CNUM);
-------------------------------------------------------------------------------------------------------
UPDATE SALESPEOPLE
SET COMM=COMM*1.2
WHERE SNUM=
(SELECT SNUM FROM ORDERS
GROUP BY SNUM
HAVING SUM(AMT)>3000);
